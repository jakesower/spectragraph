{
  "name": "@polygraph/memory-store",
  "version": "0.0.2",
  "description": "In-memory polygraph store.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "test": "NODE_ENV=test ava",
    "test:watch": "NODE_ENV=test ava -w"
  },
  "keywords": [],
  "author": "Jake Sower",
  "license": "CC BY-NC-SA",
  "dependencies": {
    "@polygraph/schema-utils": "^0.0.3",
    "@polygraph/utils": "^0.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "ava": "^2.4.0"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "test/*.test.js"
    ],
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ]
  },
  "publishConfig": {
    "access": "public"
