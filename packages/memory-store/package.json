{
	"name": "@spectragraph/memory-store",
	"version": "0.2.0",
	"type": "module",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.esm.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.esm.js",
			"require": "./dist/index.cjs.js"
		}
	},
	"scripts": {
		"build": "npm run clean && rollup -c && cp index.d.ts dist/",
		"clean": "rm -rf dist",
		"format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" \"test/**/*.{js,jsx,ts,tsx,json,css,md}\" \"README.md\"",
		"format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" \"test/**/*.{js,jsx,ts,tsx,json,css,md}\" \"README.md\"",
		"lint": "eslint src/ test/",
		"lint:fix": "eslint --fix src/ test/",
		"prepublishOnly": "npm run lint && npm run test",
		"test": "NODE_ENV=test vitest run --coverage",
		"test:watch": "vitest"
	},
	"keywords": [],
	"author": "Jake Sower",
	"license": "AGPL-3.0",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^28.0.9",
		"@rollup/plugin-node-resolve": "^16.0.3",
		"@spectragraph/interface-tests": "*",
		"@vitest/coverage-istanbul": "^4.0.3",
		"@vitest/coverage-v8": "^4.0.3",
		"rollup": "^4.52.5",
		"vitest": "^4.0.3"
	},
	"publishConfig": {
		"access": "public"
	},
	"private": false,
	"files": [
		"dist/",
		"README.md",
		"LICENSE"
	],
	"sideEffects": false,
	"dependencies": {
		"@spectragraph/core": "*",
		"es-toolkit": "^1.41.0",
		"json-expressions": "^0.15.0"
	}
}
