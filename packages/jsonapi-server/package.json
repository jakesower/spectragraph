{
	"name": "@spectragraph/jsonapi-server",
	"version": "0.0.12",
	"type": "module",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.esm.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.esm.js",
			"require": "./dist/index.cjs.js"
		}
	},
	"scripts": {
		"build": "npm run clean && rollup -c && cp index.d.ts dist/",
		"clean": "rm -rf dist",
		"format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" \"test/**/*.{js,jsx,ts,tsx,json,css,md}\" \"README.md\"",
		"format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" \"test/**/*.{js,jsx,ts,tsx,json,css,md}\" \"README.md\"",
		"start": "tsx test/test-server.js",
		"lint": "eslint src/ test/",
		"lint:fix": "eslint --fix src/ test/",
		"test": "start-test 3000 'NODE_ENV=test vitest run --no-file-parallelism --coverage'",
		"test:unit": "NODE_ENV=test vitest run test/unit/ --coverage",
		"test:watch": "vitest"
	},
	"keywords": [],
	"author": "Jake Sower",
	"devDependencies": {
		"@spectragraph/memory-store": "*",
		"@rollup/plugin-commonjs": "^28.0.6",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^16.0.2",
		"ajv-formats": "^3.0.1",
		"axios": "^1.12.2",
		"rollup": "^4.52.4",
		"start-server-and-test": "^2.1.2",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.0",
		"vite": "7.1.9",
		"vitest": "^3.2.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"private": true,
	"files": [
		"dist/",
		"README.md",
		"LICENSE"
	],
	"sideEffects": false,
	"dependencies": {
		"@spectragraph/core": "*",
		"@spectragraph/utils": "*",
		"ajv": "^8.17.1",
		"body-parser": "^2.2.0",
		"es-toolkit": "^1.40.0",
		"express": "^5.1.0",
		"json-expressions": "^0.12.0",
		"json5": "^2.2.3"
	}
}
